{
  "name": "fast-analytics-js",
  "version": "0.2.1",
  "description": "SDK для отправки логов и ошибок в Fast Analytics",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src",
    "check-types": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "analytics",
    "error-tracking",
    "logging",
    "monitoring",
    "sdk",
    "js",
    "javascript",
    "fast-analytics"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "eslint": "^9.39.1",
    "tsup": "^8.3.5",
    "typescript": "^5.9.2"
  }
}
