{
  "name": "fast-analytics-js",
  "version": "0.6.5",
  "description": "SDK для отправки логов и ошибок в Fast Analytics",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src",
    "check-types": "tsc --noEmit",
    "test": "vitest --silent --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "analytics",
    "error-tracking",
    "logging",
    "monitoring",
    "sdk",
    "js",
    "javascript",
    "fast-analytics"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "html2canvas-pro": "^1.4.1"
  },
  "peerDependenciesMeta": {
    "html2canvas-pro": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.39.1",
    "happy-dom": "^15.11.6",
    "html2canvas-pro": "^1.4.1",
    "tsup": "^8.3.5",
    "typescript": "^5.9.2",
    "vitest": "^2.1.8"
  }
}
